<Application x:Class="mcrcon.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 Startup="Application_Startup"
			 Exit="Application_Exit">
    <Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary Source="pack://application:,,,/UniversalControls;component/Themes/Light.xaml" />
				<ResourceDictionary Source="pack://application:,,,/UniversalControls;component/Themes/Colors/Blue.xaml" />
				<ResourceDictionary Source="pack://application:,,,/UniversalControls;component/Controls.xaml" />
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
    </Application.Resources>
</Application>
